{
  // "launch.vs.json" with reasonable defaults for CMake integration with Visual Studio 2017
  // -> See https://blogs.msdn.microsoft.com/vcblog/2016/10/05/cmake-support-in-visual-studio/ for more information about this file.
  "version": "0.2.1",
  "defaults": {},
  "configurations": [
    {
      "type": "default",
      "project": "CMakeLists.txt",
      "projectTarget": "Examples.exe (Install)",
      "name": "Examples.exe (Install with Arguments)",
      "args": [ "ImGuiExampleSelector -r Direct3D11" ]
    },
    {
      "type": "default",
      "project": "CMakeLists.txt",
      "projectTarget": "ExampleProjectCompiler.exe (Install)",
      "name": "ExampleProjectCompiler.exe (Install with Arguments)",
      "args": [ "Direct3D11_50" ]
    }
  ]
}